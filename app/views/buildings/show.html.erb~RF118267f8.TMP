<div class="page-header">
  <h1><%= @building.name %></h1>
</div>

<dl class="dl-horizontal">
  <dt>Photo</dt>
  <dd><%= @building.image_url %></dd>

  <dt>Name</dt>
  <dd><%= @building.name %></dd>

